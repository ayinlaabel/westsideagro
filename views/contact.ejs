<%- include ./partials/header %>
<section class="contact">
    <div class="contact-us">
      <div class="wp-wrap">
        <div class="vc-empty-box-1"></div>
        <div class="contact-us-content">
          <div class="contact-us-content-wrapper">
            <div class="contact-us-info">
              <div class="vc-empty-space" style="height: 0.9rem;"></div>
              <div class="vc-empty-space height-large" style="height: 32px;"></div>
              <div class="contact-title">
                <h6 class="contact-item-subtitle">Contact info</h6>
                <h2 class="contact-item-title">Get in Touch with Us</h2>
              </div>
              <div class="vc-empty-space vc-custom height-medium" style="height: 32px"></div>
              <div class="vc-text-column vc-custom-element vc-custom-ad">
                <div class="wbp-wrapper">
                  <p>
                    <span class="contact-margin">Address:</span>25/27, Raymond Street, Yaba
                    <br>
                    <span class="contact-margin">Phone:</span> <a href="tel: ">+234 809 235 8605</a>
                    <br>
                    <span class="contact-margin">Email:</span> <a
                      href="mailto:johndoe@example.com ">johndoe@example.com</a>
                  </p>
                </div>
              </div>
              <div class="vc-emplty-space height-medium" style="height: 32px;"></div>
            </div>
            <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#staticBackdrop">
                Leave a Message
            </button>
          </div>
        </div>
        <div class="vc-empty-box-2"></div>
      </div>

    </div>
    <div class="contact-map">
      <div class="wp-wrapp">
        <div class="contact-map-content vc-google-map" style="width:100%;height:610px;" data-zoom="16"
          data-style="grey">
          <iframe
            src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d1982.0522012783438!2d3.3735828620864514!3d6.508467190574075!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x103b8da3ec4d26a1%3A0xace9335b75c3abc3!2skraneabel!5e0!3m2!1sen!2sng!4v1614872121153!5m2!1sen!2sng"
            scrolling="no" marginheight="0" marginwidth="0" frameborder="0" aria-label="One"
            class="wizors_investments_resize"></iframe>
        </div>
      </div>
    </div>
  </section>

  <!-- Modal -->
<div class="modal fade" id="staticBackdrop" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="staticBackdropLabel">Contact us</h5>
          <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        </div>
        <div class="modal-body">
          ...
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Cancel</button>
          <button type="button" class="btn btn-primary">Send Message</button>
        </div>
      </div>
    </div>
  </div>
  <%- include ./partials/footer %>